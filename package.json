    "bulma": "^0.7.1",
    "cross-env": "^5.2.0",
    "laravel-mix": "^2.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "trumbowyg": "^2.10.0"
  }
